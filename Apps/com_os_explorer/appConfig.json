{
    "appname": "Explorador de Archivos",
    "applogo": "app.png",
    "version": "1.0.0",
    "index": "index.html",
    "width": 800,
    "height": 600,
    "minWidth": 400,
    "minHeight": 300,
    "resizable": true,
    "navMenus": [
      {
          "name": "Ventana",
          "items": [
              {
                  "type": "addNewContextualBtn",
                  "name": "Maximizar ventana",
                  "function": "maximizeWindow(pid)"
              },
              {
                  "type": "addNewContextualBtn",
                  "name": "Maximizar ventana sin dock",
                  "function": "maximizeWindowWithoutDock(pid)"
              },
              {
                  "type": "addNewContextualBtn",
                  "name": "Ventana completa",
                  "function": "fullScreenWindow(pid)"
              },
              {
                  "type": "addNewContextualBtn",
                  "name": "Minimizar ventana",
                  "function": "minimizeWindow(pid)"
              }
          ]
      }
  ]
}